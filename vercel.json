{
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "./(.*)",
            "dest": "/"
        }
    ],
    "rewrites": [{ "source": "/api/(.*)", "destination": "/api" }, { "source": "/results", "destination": "/public/results.html" }]    
}